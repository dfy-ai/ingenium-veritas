{
  "name": "ingenium-veritas",
  "main": "src/server/index.ts",
  "compatibility_date": "2025-10-08",
  "kv_namespaces": [
    {
      "binding": "memy",
      "id": "4a9bfbffb4d440cba86badc508e2045a"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "name": "Chat",
        "class_name": "ingenium-veritas-Chat"
      }
    ]
  },
  "ai": {
    "binding": "AI"
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["ingenium-veritas-Chat"]
    }
  ],
  "assets": {
    "directory": "./public",
    "binding": "ASSETS",
    "not_found_handling": "single-page-application"
  },
  "build": {
    "command": "esbuild src/client/index.tsx --bundle --splitting --format=esm --platform=browser --outdir=public/dist"
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}